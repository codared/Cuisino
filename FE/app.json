{
  "expo": {
    "scheme": "abuad-eats",
    "name": "Cuisino",
    "slug": "Cuisino",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/delivery.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/delivery.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jose.szno.Cuisino",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/delivery.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jose.szno.Cuisino"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "b664c392-266e-4fed-9953-21fc23512055"
      }
    }
  }
}
